    "parcel-plugin-static-files-copy": "^2.5.0",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "assets": "^3.0.1",
    "three": "^0.122.0"
